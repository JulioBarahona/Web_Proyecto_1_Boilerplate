{
  "name": "web_proyecto_1_boilerplate",
  "version": "1.0.0",
  "description": "boilerpalte that will help set up future personal proyects in less time, with less effort and lower the chances of downloading very useful packages all the time",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-server && npm run build-client",
    "build-server": "babel src/server --out-dir ./dist",
    "build-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "node ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JulioBarahona/Web_Proyecto_1_Boilerplate.git"
  },
  "keywords": [
    "boilerplate",
    "react",
    "web",
    "development"
  ],
  "author": "Julio Barahona M",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JulioBarahona/Web_Proyecto_1_Boilerplate/issues"
  },
  "homepage": "https://github.com/JulioBarahona/Web_Proyecto_1_Boilerplate#readme",
  "dependencies": {
    "express": "^4.16.4",
    "html-webpack-plugin": "^3.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3"
  }
}
