{
  "name": "web_proyecto_1_boilerplate",
  "version": "1.0.0",
  "description": "boilerpalte that will help set up future personal proyects in less time, with less effort and lower the chances of downloading very useful packages all the time",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "build-babel": "npm run build-babel-server && npm run build-babel-client",
    "build-babel-server": "babel src/server --out-dir ./dist",
    "build-babel-client": "babel src/client --copy-files --out-dir ./dist/public",
    "start": "node ./dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JulioBarahona/Web_Proyecto_1_Boilerplate.git"
  },
  "keywords": [
    "boilerplate",
    "react",
    "web development"
  ],
  "author": "Julio Barahona M",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JulioBarahona/Web_Proyecto_1_Boilerplate/issues"
  },
  "homepage": "https://github.com/JulioBarahona/Web_Proyecto_1_Boilerplate#readme",
  "dependencies": {
    "autoprefixer": "^9.3.1",
    "babel-core": "^6.26.3",
    "babel-preset-es2017": "^6.24.1",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "express": "^4.16.4",
    "file-loader": "^2.0.0",
    "path": "^0.12.7",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "redux": "^4.0.1",
    "rimraf": "^2.6.2",
    "saas": "^1.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.26.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "react-test-renderer": "^16.6.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
